<?xml version='1.0' encoding='utf-8'?>
<widget id="net.rogerlsmimth.hybridconcept" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>hybridConcept</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="rogerlsmith@live.com" href="http://rogerlsmith.net">
        Roger L. Smith
    </author>
    <content src="index.html" />
    <access origin="*" />
	<feature name="File">
	   <param name="android-package" value="org.apache.cordova.FileUtils" />
	</feature>
	<feature name="FileTransfer">
	   <param name="android-package" value="org.apache.cordova.FileTransfer" />
	</feature>
</widget>
